{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "buildJavascriptBundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "removeResourcesFolders": "rm -rf ./android/app/src/main/res/drawable*/ && rm -rf ./android/app/src/main/res/raw/",
    "buildDebug": "npm run buildJavascriptBundle && cd android && ./gradlew app:assembleDebug && cp app/build/outputs/apk/debug/app-debug.apk \"../firedefence.apk\" && cd ..",
    "release-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd ..",
    "buildRelease": "npm run buildJavascriptBundle && cd android && ./gradlew app:assembleRelease && cp app/build/outputs/bundle/release/app-release.aab \"../firedefence.app\" && cd .."
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^10.1.0",
    "@react-navigation/drawer": "^5.11.4",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^8.3.0",
    "core-decorators": "^0.20.0",
    "lodash": "^4.17.21",
    "mobx": "^5.15.7",
    "mobx-react": "^6",
    "moment": "^2.29.1",
    "querystring": "^0.2.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-accordion": "^1.0.1",
    "react-native-collapsible": "^1.6.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-image-crop-picker": "^0.35.1",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-input-prompt": "^1.0.0",
    "react-native-notifications": "^3.4.1",
    "react-native-permissions": "^3.0.0",
    "react-native-prompt": "^1.0.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.0",
    "react-native-vector-icons": "^7.1.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
